{
    "name": "beyondcode/fathom-api",
    "description": "Fathom Analytics API",
    "keywords": [
        "beyondcode",
        "fathom-api"
    ],
    "homepage": "https://github.com/beyondcode/fathom-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "marcel@beyondco.de",
            "homepage": "https://beyondcode.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "nesbot/carbon": "^2.31",
        "symfony/browser-kit": "^5.1",
        "symfony/css-selector": "^5.2",
        "symfony/http-client": "^5.1",
        "symfony/mime": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "BeyondCode\\FathomAnalytics\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeyondCode\\FathomAnalytics\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
